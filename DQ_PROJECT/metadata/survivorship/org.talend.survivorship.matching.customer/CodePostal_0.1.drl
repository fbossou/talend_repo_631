package org.talend.survivorship.matching.customer 

rule "CodePostal"
    no-loop true
    dialect "mvel"
    ruleflow-group "CodePostalGroup"
when
    $input : RecordIn( codepostal matches "\\d{5}"  )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "CodePostal", drools.getRule().getName());

end


