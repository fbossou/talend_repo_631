package org.talend.survivorship.matching.customer 

rule "Prenom"
    no-loop true
    dialect "mvel"
    ruleflow-group "PrenomGroup"
when
    $input : RecordIn( dataset.isMostCommon( prenom, "Prenom", false) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "Prenom", drools.getRule().getName());

end


